		<script type="text/javascript" lazy="false"><![CDATA[
scwin.flag = false;

/**
 * @event
 * @name onpageload
 * @description 초기 스크립트 수행 함수 정의
 */
scwin.onpageload = function () {
	const cookie_val = $c.win.getLangCode();
	sel_multiLang.setValue(cookie_val);
	if (cookie_val != "en") {
		scwin.flag = true;
	}
	dlt_sample.insertRow()
};

/**
 * @event
 * @name sel_multiLang_onchange
 * @description 국가별 언어 설정을 한다.
 */
scwin.sel_multiLang_onchange = function () {
	if (scwin.flag) {
		$c.win.setLangCode(sel_multiLang.getValue());
		$c.win.reload();
	} else {
		scwin.flag = true;
	}
};

/**
 * @event
 * @name wfTitleDesc_btn_DescTitleBtn_onclick
 * @description 예제설명1의 toggle을 조작한다.
 */
scwin.wfTitleDesc_btn_DescTitleBtn_onclick = function() {
	this.getParent().getParent().toggleClass("on")
};

/**
 * @event
 * @name btn_trigger1_onclick
 * @description 탭을 추가한다.
 */
scwin.btn_trigger1_onclick = async function(e) {
	const tabObj = { 
		closable : true, //탭 닫기 기능 제공
		openAction : "", // exist 는 기존 탭을 갱신, new 는 항상 새로, select는 동일 id 가 존재하면 선택, last: 기존 tab을 마지막 tab으로 이동후 선택
		label : "menuNm" ,
		useLocale: true,
		localeRef: "MSG_CM_00056" //엔진 반영 예정
	};

	const contObj = {
		frameMode : "wframePreload",
		scope : true,
		src : "/ui/SP/Parameter/DataObject_ParameterSend.xml",
		alwaysDraw : true,
		title : "title"  ,
		dataObject : { 
			type : "json", 
			name : "paramData", 
			data : {},
		}
	};
    
	const tabId = await tac_tabControl1.addTab("sample", tabObj, contObj);
	tac_tabControl1.setSelectedTabIndex(tabId);
	return tabId;
	
};

/**
 * @event
 * @name btn_trigger2_onclick
 * @description 윈도우 탭을 추가한다.
 */
scwin.btn_trigger2_onclick = function(e) {
	wdc_windowContainer1.createWindow( "edu001" , null , "/ui/SP/Parameter/Popup_Parameter.xml" );
};
]]></script>
